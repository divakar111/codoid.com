/* Common */
@font-face {
  font-family: 'segoeui';
  src: url('../fonts/segoeui.eot?#iefix') format('embedded-opentype'),  url('../fonts/segoeui.otf')  format('opentype'),
	     url('../fonts/segoeui.woff') format('woff'), url('../fonts/segoeui.ttf')  format('truetype'), url('../fonts/segoeui.svg#segoeui') format('svg');
  font-weight: normal;
  font-style: normal;
}
@font-face {
	font-family: "segoeuilight";
	src: url("../fonts/segoeuil.eot");
	src: url("../fonts/segoeuil.eot#iefix") format("embedded-opentype"),
	url("../fonts/segoeuil.woff") format("woff"),
	url("../fonts/segoeuil.ttf") format("truetype"),
	url("../fonts/segoeuil.svg") format("svg");
	font-weight: normal;
	font-style: normal;
}
@font-face {
	font-family: "segoeuibold";
	src: url("../fonts/segoeuib.eot");
	src: url("../fonts/segoeuib.eot#iefix") format("embedded-opentype"),
	url("../fonts/segoeuib.woff") format("woff"),
	url("../fonts/segoeuib.ttf") format("truetype"),
	url("../fonts/segoeuib.svg") format("svg");
	font-weight: normal;
	font-style: normal;
}

/*primary-color - #3387df*/

body {   font-family: 'segoeui'; background:#fff; color:#4f4f4f;
-webkit-font-smoothing: none;
-webkit-font-smoothing: subpixel-antialiased;
-webkit-font-smoothing: antialiased; font-weight:normal;}
h1, h2, h3, h4, h5, h6, .h1, .h2, .h3, .h4, .h5, .h6 { font-family: 'segoeuibold';text-transform:uppercase;}
ul li { list-style:none;}
a:focus {outline:none; border:none; box-shadow:none;}
.btn {}
.btn:active { box-shadow:none;}
.modal-dialog {position:relative;}
.modal-dialog .close { position:absolute; top:-11px; right:-10px; z-index:1; color:#fff;opacity:1; background:#333; border-radius:20px; width:23px; height:23px; font-size:14px;}
a { color:#28ace2;}
a:hover { color:#28ace2; text-decoration:underline;}
a:focus { color:#28ace2; text-decoration:none;}


/* Header */

.header-top { background:#f3f3f3;}
.header-top .social ul li { float:left; display:inline-block;}
.header-top .social a { padding:8px; display:inline-block;} 
.header-top .social a i { color:#555555; font-size:20px;} 
.header-top .social a:hover i { color:#3387df;}
.header-top .support ul { float:right; margin-bottom:0px;}
.header-top .support ul li {float:left; display:inline-block; border-left:1px solid #dddddd;}
.header-top .support ul li a { padding:9px 12px; display:inline-block; color:#bbbbbb;}
.header-top .support ul li a:hover {color:#3387df; text-decoration:none;}
.header-top .support ul li:last-child a{ padding-right:5px;}
.header-top .support ul li a .fa { margin-right:9px;}
.header-bottom { padding:15px 22px 0px; background-color:#fff; height: 88px;}

.navbar-inverse { background:none; border:none;}
.navbar-nav { padding-top:28px;font-family: "segoeuilight";}
.navbar-inverse .navbar-nav { float:right;}
.navbar-inverse .navbar-nav > li { margin:0px 10px; }
.navbar-inverse .navbar-nav > li > a { color:#000;}
.navbar-nav > li > a { padding:4px; text-transform:uppercase; font-size:20px;}
.navbar-inverse .navbar-nav > li > a:hover { border-bottom:2px solid #3387df; color:#4f4f4f;}
.navbar-inverse .navbar-nav > .active > a, .navbar-inverse .navbar-nav > .active > a:hover, .navbar-inverse .navbar-nav > .active > a:focus { background:none; border-bottom:2px solid #3387df; color:#4f4f4f; font-weight:bold;}
.navbar-toggle { background:#3387df; border:none;}

.navbar {margin-bottom: 0px !important ;}
.carousel-inner > .item > img, .carousel-inner > .item > a > img { width:100%;}
.carousel-control.left { background:none;}
.carousel-control.right { background:none;}
.carousel-control .icon-prev, .carousel-control .icon-next, .carousel-control .glyphicon-chevron-left, .carousel-control .glyphicon-chevron-right { width:40px; background:rgba(0,0,0,0.9); height:40px;}
.glyphicon-chevron-left::before { content:inherit;}
.glyphicon-chevron-right::before { content:inherit;}
.glyphicon-chevron-left .fa { font-size:26px; margin-top:6px; margin-left:-3px;}
.glyphicon-chevron-right .fa { font-size:26px; margin-top:6px;margin-right:-3px;}
.carousel-control { width:auto; left:0px;}
.carousel-caption { text-align:left; width:30%; top:43%; left:8%;}
.carousel-caption h3 { background:rgba(51,135,223,0.7); font-size:34px; text-transform:uppercase; color:#fff; padding:20px 12px; margin:0px; text-shadow:none; font-weight:bold;}
.carousel-caption p { font-family: "segoeuilight";background:rgba(51,51,51,0.7); font-size:15px; text-transform:uppercase; color:#fff; padding:12px;}

.home-blurb { text-align:center; padding:30px 60px;}
.home-blurb p { font-size:20px; padding:24px 100px;font-family: "segoeuilight"; line-height:24px;}
.home-blurb h1 { font-weight:bold; padding:12px 25px 16px 25px; border-bottom:1px solid #ccc; display:inline-block; margin:0px auto; font-size:35px; }

.home-panel { padding:0 0px 40px 20px;}
.home-panel .col { border:1px solid #eee; padding:20px; margin:0px 35px; width:27%; /*width:30%;*/ }  
.home-panel .col-2 { border:1px solid #eee; padding:20px;} 
.home-panel .col-3 { border:1px solid #eee; padding:20px;} 
.home-panel .col h2 { font-size:16px;}
.home-panel .col p { font-size:16px; margin-bottom:0px; font-family: 'segoeui';}
.home-panel .col div { text-align:center;}
.home-panel .col .fa { font-size:36px; color:#999999;}
.home-panel .col .btn { background:#3387df; color:#fff; border-radius:0px; border:none; text-transform:uppercase; margin-top:20px; padding:6px 34px; font-size:13px;text-shadow: 0 0 1px rgba(0,0,0,0.3);}
.home-panel .col .btn:hover { background:#4f4f4f;}
.footer-wrap { background:#4f4f4f; color:#fff; padding:15px 0px;font-family: "segoeuilight";}
.footer-wrap ul { margin:10px 0 0 0; padding:0px;}
.footer-wrap ul li { float:left;}
.footer-wrap ul li a { color:#fff; padding:10px 30px 20px 0px; font-size:18px; display:inline-block;}
.footer-wrap ul li a:hover { text-decoration:none; color:#3387df;}
.footer-wrap h2 { font-weight:normal; font-size:36px; margin-bottom:20px;font-family: "segoeuilight";}
.footer-wrap p { font-size:18px; padding-right:15px;}
.footer-wrap .copyright { padding:0px 0 0 10px;}
.footer-wrap .bottom { border-top:1px solid #333; margin-top:10px;}
.header-top .social ul { padding:0px;}

.services-blurb{text-align:center; padding-top: 204px;}
.consulting{color:#3387df;}
.services-panel { /*padding:0 0px 40px 20px;*/ /*padding: 0 0px 50px 5px;*/ padding: 0px 0px 47px 10px;}
.services-panel .col { border:1px solid #eee; padding:20px; margin:0px 10px; width:97%; padding-right: 20px; }  
.services-panel .col-2 { border:1px solid #eee; padding:20px;} 
.services-panel .col-3 { border:1px solid #eee; padding:20px;} 
.services-panel .col h2 { font-size:16px; color:#fff; text-align:center;}
.services-panel .col p { font-size:16px; color:#fff;}
.services-panel .col div { text-align:center;}
.services-panel .col .fa { font-size:36px; color:#fff;}
.services-panel .col .btn { background:transparent; color:#fff; border:1px solid #fff; border-radius:0px; border-bottom:3px solid #fff; margin-bottom: 6px; border-left: 3px solid #fff;  text-transform:uppercase; margin-top:20px; padding:6px 34px; font-size:13px;text-shadow: 0 0 1px rgba(0,0,0,0.3);}
.services-panel .col .btn:hover { background:#4f4f4f;}
